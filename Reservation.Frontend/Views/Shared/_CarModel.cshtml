@model Reservation.Frontend.Models.TyreChangeReservationFormModel

@Html.LabelFor(x => x.CarModel, new {@class = "form-label"})
<input asp-for="CarModel" type="text" class="form-control size-auto" />
@{
    if (Model.IsPost)
    {
        <span asp-validation-for="CarModel" class="text-danger"></span>
    }
}